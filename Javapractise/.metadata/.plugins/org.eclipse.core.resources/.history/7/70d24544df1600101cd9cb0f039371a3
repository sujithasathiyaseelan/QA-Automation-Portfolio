package xyz;
import xyz.Encapsulation;

public class Encap2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Encapsulation account = new Encapsulation(345678, "BETH", "FD");

        // Accessing data using getters
        System.out.println("Account Holder Name: " + account.getName());
        System.out.println("Account Number: " + account.getAccountNo());
        System.out.println("Deposit Type: " + account.getTypeOfDeposit());
}




}




	

}
